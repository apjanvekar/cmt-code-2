<h1>Listing settings</h1>

<table>
  <tr>
  <% for column in Setting.content_columns %>
    <th><%= column.human_name %></th>
  <% end %>
  </tr>
  
<% for setting in @settings %>
  <tr>
  <% for column in Setting.content_columns %>
    <td><%=h setting.send(column.name) %></td>
  <% end %>
    <td><%= link_to 'Edit', :action => 'edit', :id => setting %></td>
  </tr>
<% end %>
</table>


<br />


