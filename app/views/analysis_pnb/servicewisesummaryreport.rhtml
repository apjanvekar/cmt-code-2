								end %>
								
								<%= "#{hours}:#{minutes}:#{seconds}"  %>
                          <% end %>
                       <% end %>  

				&nbsp;</font></td>
														
         </tr>
	     <% end %> 
		 <% end %>
					   
      	<tr>
