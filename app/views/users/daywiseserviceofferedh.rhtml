     <%  @t=Transact.find_by_sql("select distinct(serviceid) as Service_ID,transdate as Date, count(tokenno) as
                                    tokenno        
                                 from transactmaster 
                                 where transdate<='#{params[:enddate]}'and transdate>='#{params[:startdate]}' 
                                 and tokenstatus=1
                                 group by serviceid
                                 order by transdate ")
        
        
  @t.each do |c| %>
  <tr align=center>
<td nowrap  height="11" align=center ><font size="2" face="Arial">
<%= c.Date %></font></td> 

<% @b= Service.find_first(["serviceid = ? ",c.Service_ID]) %>
<td nowrap  height="11" align=left ><font size="2" face="Arial">
<%= @b.servicename %></font></td>

<td nowrap  height="11" align=center ><font size="2" face="Arial">
<%= c.tokenno %></font></td>

<% end %>
